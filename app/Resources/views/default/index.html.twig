{% extends 'base.html.twig' %}


{% block body %}
    <div class="col-md-6 col-md-offset-3">
        <h2 class="text-center">Alquiler de Vehiculos</h2>
        <div class="progress">
            <div class="progress-bar" role="progressbar" style="width:10%">
            </div>
        </div>
        <form>
            <div class="form-group">
                <label for="exampleFormControlInput1">Fecha Inicial</label>
                <input type="email" class="form-control" id="exampleFormControlInput1" placeholder="name@example.com">
            </div>
            <div class="form-group">
                <label for="exampleFormControlSelect1">Example select</label>
                <select class="form-control" id="exampleFormControlSelect1">
                    <option>1</option>
                    <option>2</option>
                    <option>3</option>
                    <option>4</option>
                    <option>5</option>
                </select>
            </div>
            <div class="form-group">
                <label for="exampleFormControlSelect2">Example multiple select</label>
                <select multiple class="form-control" id="exampleFormControlSelect2">
                    <option>1</option>
                    <option>2</option>
                    <option>3</option>
                    <option>4</option>
                    <option>5</option>
                </select>
            </div>
            <div class="form-group">
                <label for="exampleFormControlTextarea1">Example textarea</label>
                <textarea class="form-control" id="exampleFormControlTextarea1" rows="3"></textarea>
            </div>
            <div class="form-group">
                <button type="submit" name="enviar" class="btn btn-success"><i class="fa fa-check-square-o"></i> Continuar
                </button>
                <a href="#" class="btn btn-info"><i class="fa fa-arrow-circle-left"
                                                    aria-hidden="true"></i> Volver</a>
            </div>
        </form>
    </div>
{% endblock %}

